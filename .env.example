# Anthropic API Key
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key_here

# Vision Provider (anthropic or openai)
VISION_PROVIDER=anthropic

# Fallback Provider (optional, if primary provider fails)
FALLBACK_PROVIDER=openai

# Logging Level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Anthropic Model (default: claude-3.5-sonnet-beta)
ANTHROPIC_MODEL=claude-3.5-sonnet-beta

# OpenAI Model (default: gpt-4o-mini)
OPENAI_MODEL=gpt-4o-mini
OPENAI_BASE_URL= # Optional: Set a custom base URL for the OpenAI API
OPENAI_TIMEOUT=60 # Optional: Set a custom timeout (in seconds) for the OpenAI API

# Tesseract OCR Settings
ENABLE_OCR=false # Set to 'true' to enable Tesseract OCR text extraction
TESSERACT_CMD= # Optional: Set custom path to Tesseract executable (e.g. C:\Program Files\Tesseract-OCR\tesseract.exe)
