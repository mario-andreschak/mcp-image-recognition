# Vision Provider (anthropic, openai, or cloudflare)
VISION_PROVIDER=openai

# Fallback Provider (optional, if primary provider fails)
FALLBACK_PROVIDER=

# Logging Level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=ERROR

# Anthropic Settings
# ANTHROPIC_API_KEY=aaaaaaa
# ANTHROPIC_MODEL=claude-3.5-lates 

# OpenAI Settings
# OPENAI_TIMEOUT=60

OPENAI_API_KEY=gggggggggg
OPENAI_BASE_URL=
OPENAI_MODEL=gpt-4o-mini

# Optional: Set a custom base URL/Model for the OpenAI API
# - openrouter
# OPENAI_API_KEY=ooooooo
# OPENAI_BASE_URL=https://openrouter.ai/api/v1
# OPENAI_MODEL=anthropic/claude-3.5-sonnet:beta
# - grok
# OPENAI_API_KEY=xxxxxxx
# OPENAI_BASE_URL=https://api.x.ai/v1
# OPENAI_MODEL=grok-2-vision-latest

# Cloudflare Workers AI Settings
# CLOUDFLARE_API_KEY=cccccccccc
# CLOUDFLARE_ACCOUNT_ID=11111111111111111111111111111111
# CLOUDFLARE_MODEL=@cf/llava-hf/llava-1.5-7b-hf
# CLOUDFLARE_MAX_TOKENS=512
# CLOUDFLARE_TIMEOUT=60

# Tesseract OCR Settings
# Set to 'true' to enable Tesseract OCR text extraction
# ENABLE_OCR=false 
# Path to Tesseract executable
# TESSERACT_CMD= 
